QMainWindow{
background:#FDFEFF;border:none;
}

QMenuBar QMenu{
	background:#CFDBEB;
	color:#454655;
	font: 700 10pt "Segoe UI";
}

QMenuBar QMenu::separator{
background:#454655;
height:2px;
}

QMenuBar QMenu::item{
padding: 5px;
}

QMenuBar QMenu::item:selected{
	background:rgb(221, 233, 250);
}


QMenuBar{
	background:#454655;
	color:#CFDBEB;
	font: 700 10pt "Segoe UI";
}

QMenuBar::item{
border-right: 1px solid #CFDBEB;
padding:5px 10px;
}

QMenuBar::item:selected{
background:rgb(87, 88, 107);
}

QMenuBar::item:pressed {
    background: #CFDBEB;
	color:#454655;
}


QToolBar{
background:#CFDBEB;border:none;
padding: 10px;
}

QToolButton{
width:50px;
height:35px;
border:none;
}

QToolButton::hover{
background:rgb(221, 233, 250);
}

QToolButton::pressed{
background:rgb(200, 212, 227);
}

QToolBar::separator{
background:#454655;
border:none;
ouline:0;
width:2px;
}


QTabWidget{
outline:0;
background:green;
}

QTabWidget::pane { /* The tab widget frame */
outline:0;
	background:#CFDBEB;
border:none;
}



/* Style the tab using the tab sub-control. Note that
    it reads QTabBar _not_ QTabWidget */
QTabBar::tab {
    background: #454655;
    padding: 5px 10px;
	font: 700 10pt "Segoe UI";
	color:#CFDBEB;
	outline:0;
	border:none;
}


QTabBar::tab:selected {
    background: #CFDBEB;
color:#454655;
}

QTabBar::tab:!selected:hover {
	background:rgb(87, 88, 107);
}

QTabBar::tab:selected:hover {
	background:rgb(221, 233, 250);
}





QListWidget{
outline: 0;
font: 700 12pt "Segoe UI";
background:white;border:none;
}

QListWidget::item{

background:rgb(54, 48, 98);
height: 40px;
padding:20px;
border-bottom: 2px solid white;
color:white;
}


QListWidget::item::hover{
background-color:white;
color:rgb(38, 70, 83);
border-bottom: 2px solid rgb(38, 70, 83);
}



QTextEdit{
color:#454655;
font: 700 12pt "Segoe UI";
background:#FDFEFF;border:none;
margin:3px;
padding:10px;
}

QScrollBar:vertical {
     border: 2px solid #454655;
     background: #CFDBEB;
     width: 15px;
     margin: 22px 0 22px 0;
 }
 QScrollBar::handle:vertical {
     background: white;
     min-height: 20px;
 }
 QScrollBar::add-line:vertical {
     border: 2px solid #454655;
     background: #CFDBEB;
     height: 20px;
     subcontrol-position: bottom;
     subcontrol-origin: margin;
 }

 QScrollBar::sub-line:vertical {
     border: 2px solid #454655;
     background: #CFDBEB;
     height: 20px;
     subcontrol-position: top;
     subcontrol-origin: margin;
 }
 QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     border: 2px solid #454655;
     width: 3px;
     height: 3px;
     background: #fdfeff;
 }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
     background: none;
 }



