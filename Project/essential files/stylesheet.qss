}

QMessageBox{
	background:#FDFEFF;border:none;
}


QMessageBox Qlabel {
	color: #454655;
	font: 700 12pt "Segoe UI";
}





QCheckBox {
	padding:2px;
	color:#454655;
}
QCheckBox:hover {
	border-radius:4px;
	padding: 1px;
	color:#CFDBEB;
	background-color:#454655;
}
QCheckBox::indicator:checked {
	border:1px solid #454655;
	border-radius:4px;
  	background-color:#CFDBEB;
}
QCheckBox::indicator:unchecked {
	border-width:1px solid rgb(246, 134, 86);
	border-radius:4px;
  	background-color:rgb(255,255,255);
}


QComboBox {
	color:#454655;
	background: #CFDBEB;
}
QComboBox:editable {
	/*when choose an item*/
	selection-color:#454655;
	selection-background-color: #CFDBEB;
}
QComboBox QAbstractItemView {
	/*selection-color: #ffffff;
	selection-background-color: red;*/
}
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
	color: #CFDBEB;
	background: #454655;
}


QRadioButton {
	padding: 1px;
}
QRadioButton::indicator:checked {
	height: 10px;
	width: 10px;
	border-style:solid;
	border-radius:5px;
	border-width: 1px;
	border-color: rgba(246, 134, 86, 255);
	color: #a9b7c6;
	background-color:rgba(246, 134, 86, 255);
}
QRadioButton::indicator:!checked {
	height: 10px;
	width: 10px;
	border-style:solid;
	border-radius:5px;
	border-width: 1px;
	border-color: rgb(246, 134, 86);
	color: #a9b7c6;
	background-color: transparent;
}


QPushButton{
background:#454655;border:none;
padding:5px 15px;
color:#CFDBEB;
	font: 700 12pt "Segoe UI";
}


QPushButton::hover{
background:#CFDBEB;border:none;
padding:5px;
color:#454655;
}


QPushButton:pressed {
background:rgb(200, 212, 227);border:none;
padding:5px;
color:#454655;
}